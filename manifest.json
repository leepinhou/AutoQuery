{
	"manifest_version": 2,
	"name": "Auto Query N",
	"version": "1.0",
	"description": "自動查詢指定網址的訊息",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"icons": {
		"16": "icons/Init.PNG",
		"48": "icons/Login-48.PNG",
		"128": "icons/Login-128.PNG"
	},
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_title": "初始化......",
		"default_icon": "icons/Init.PNG"
	},
	//--設定頁暫無
	//"options_page": "aFile.html",
	//--不了解，先停用
	"permissions": [
		"tabs", "http://*/*", "notifications"
	]
}
